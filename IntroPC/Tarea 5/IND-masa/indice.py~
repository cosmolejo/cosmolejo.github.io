#!/usr/bin/env python
import math

masa=input"ingrese la masa corporal de la persona: "
altura=imput"ingrese la estatura de la persona: "
IND=(masa/altura**2)
if (IND < 25.00 && IND > 18.50):
    printf("la persona presenta un peso normal\n");	 
else:
    if (IND >= 25.000000):
        printf("la persona presenta sobrepeso\n");  
    if (IND <= 18.500000):
        printf("la persona presenta desnutricion\n");
	  
